# Frontend Environment Variables Configuration
# Copy this file to .env.local for local development

# API Configuration
VITE_API_BASE_URL=/api/v1
VITE_BACKEND_URL=http://localhost:8000

# Development Authentication Settings
VITE_DEFAULT_USERNAME=testuser
VITE_DEFAULT_PASSWORD=testpassword
VITE_AUTO_LOGIN=false

# Development Server Settings
VITE_DEV_PORT=5173

# Test Configuration
VITE_TEST_USER_EMAIL=testuser@example.com
VITE_SKIP_INTEGRATION_TESTS=false

# Security Settings
VITE_DEV_TOKEN_PREFIX=dev-token-

# Build Settings
VITE_BUILD_MODE=development
